{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "material@0.4.3",
      "destination": "wwwroot/material/"
    },
    {
      "provider": "cdnjs",
      "library": "material@0.4.3",
      "destination": "wwwroot/material/"
    },
    {
      "provider": "cdnjs",
      "library": "materialize@1.0.0",
      "destination": "wwwroot/lib/materialize/",
      "files": [
        "css/materialize.min.css",
        "js/materialize.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bulma@0.9.2",
      "destination": "wwwroot/lib/bulma/"
    }
  ]
}